{
    "name": "bhuvidya/craft-btn-shortcodes",
    "description": "Button shortcode support in Craft v3.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "button shortcodes"
    ],
    "support": {
        "docs": "https://github.com/bhuvidya/craft-btn-shortcodes/blob/master/README.md",
        "issues": "https://github.com/bhuvidya/craft-btn-shortcodes/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "bhu Boue vidya",
            "homepage": "https://github.com/bhuvidya"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "bhuvidya\\btnshortcodes\\": "src/"
        }
    },
    "extra": {
        "name": "Button Shortcodes",
        "handle": "btn-shortcodes",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/bhuvidya/craft-btn-shortcodes/master/CHANGELOG.md",
        "components": {
            "btnShortcodes": "bhuvidya\\btnshortcodes\\services\\BtnShortcodes"
        },
        "class": "bhuvidya\\btnshortcodes\\BtnShortcodes"
    }
}
